VALIDATION_REGISTRY=<DEPLOY_CONTRACTS_TO_SEPOLIA_FIRST>

# Your testnet wallet (needs Sepolia ETH)
PRIVATE_KEY=<YOUR_TESTNET_PRIVATE_KEY>

